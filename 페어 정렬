 /* compare 함수를 만들어서 벡터의 second값에 따라 정렬하기
 bool compare(pair<int, int> a, pair<int, int> b){
  return a.second < b.second;
 }

int main(){
  int n;
  int a, b;
  int i;
  
/* 페어를 벡터의 원소로 만들어서 사용하기
  cin >> n;
  vector<pair<int, int>> v;
  for(i=0; i<n; i++){
    cin >> a >> b;
    v.push_back(make_pair(a, b));
  }
  sort(begin(), end(), greater<>());   // greater<>() 대신 less<>() 사용하면 오름차순. 기본값은 less가 생략 되어 있음. compare를 사용하면 second에 따른 오름차순.
  for(i=0; i<n; i++){
    cout << v[i].first << " " << v[i].second << endl;
  }
 */
 

 
 
  return 0;
}
